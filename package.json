{
  "name": "svelte-js",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "check": "svelte-check",
    "coverage": "vitest run --coverage",
    "test-ui": "vitest --ui",
    "lint": "eslint . --fix --max-warnings=0",
    "lint-fix": "eslint --fix",
    "format": "prettier . --write",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.6",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.18.0",
    "eslint-plugin-svelte": "^2.46.1",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "svelte": "^5.15.0",
    "svelte-check": "^4.1.3",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "svelte-check "
    ],
    "*": "prettier --ignore-unknown --write"
  }
}
